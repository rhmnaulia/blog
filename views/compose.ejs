<%- include('partials/header') -%>
<h1>Compose</h1>
<form class="composeItem" action="/compose" method="post">
    <div class="form-group">
        <label for="postTitle">Title</label>
        <input class="form-control" name="postTitle" type="text" placeholder="" autocomplete="off" />
    </div>
    <div class="form-group">
        <label for="postBody">Post</label>
        <textarea name="postBody" class="form-control" id="exampleFormControlTextarea1" rows="5" cols="30"></textarea>
    </div>
    <button class="btn btn-info" type="submit" name="button">Publish</button>

</form>

<%- include('partials/footer') -%>